declare -i count=1
declare -i start=1
numfiles=(*)
numfiles=${#numfiles[@]}

printf "" > .output
printf "#! /bin/bash\n" >> .output
if [ "$numfiles" -lt 10 ]; then
    for i in *;do
    printf "mv %s ape-2016-11-09-small-%01d.jpg\n" $i $count >> .output;
    ((count++))
    done;
elif [ "$numfiles" -lt 100 ]; then
    for i in *;do
    printf "mv %s ape-2016-11-09-small-%02d.jpg\n" $i $count >> .output;
    ((count++))
    done;
else
    for i in *;do
    printf "mv %s ape-2016-11-09-small-%03d.jpg\n" $i $count >> .output;
    ((count++))
    done;
fi
